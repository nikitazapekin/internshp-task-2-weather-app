{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["*.styled.ts", "*.styles.ts"],
      "options": {
        "bracketSameLine": false,
        "embeddedLanguageFormatting": "auto",
        "parser": "typescript"
      }
    }
  ]
}
