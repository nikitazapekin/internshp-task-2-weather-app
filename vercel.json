{
  "version": 2,
    "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "cleanDistDir": true
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ],
  "env": {
  "REACT_APP_VERCEL_TOKEN": "@REACT_APP_VERCEL_TOKEN",
  "REACT_APP_API_URL": "@REACT_APP_API_URL",
  "REACT_APP_GOOGLE_CLIENT_ID": "@REACT_APP_GOOGLE_CLIENT_ID",
  "REACT_APP_API_GOOGLE_AUTH": "@REACT_APP_API_GOOGLE_AUTH",
  "REACT_APP_API_GOOGLE_SCOPE": "@REACT_APP_API_GOOGLE_SCOPE"
}
  
}
